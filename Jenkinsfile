
pipeline {
    agent any

    stages {
        stage ("Build") {
            steps{
                echo "Build succesfull"
            }
        }
        

        stage ("Deploy") {
             steps{
                echo "Deployed  succesfull"
            }
        
            
            
        }

        stage ("Test") {
             steps{
                echo "Test Successfull"
            }
            
        }

        stage ("Release") {
             steps{
                echo " Initial version released"
            }
            
        }
    }
}
 

